<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:element name="root"/>
    <xs:complexType name="root">
        <xs:sequence>
            <xs:element name="consoleId" type="xs:string"/>
            <xs:element name="consoleName" type="xs:string"/>
            <xs:element name="manufacturer" type="xs:string"/>
            <xs:element name="releaseYear" type="xs:int"/>
            <xs:element name="sales" type="xs:float"/>
        </xs:sequence>
    </xs:complexType>
    <xs:simpleType name="releaseYear">
        <xs:restriction base="xs:int">
            <xs:minInclusive value="1900"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="sales">
        <xs:restriction base="xs:float">
            <xs:minInclusive value="0"/>
        </xs:restriction>
    </xs:simpleType>
    </xs:schema>